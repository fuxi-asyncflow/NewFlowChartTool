    <system:String x:Key="{x:Static reskeys:ResourceKeys.Menu_ConnectNodes}">connect</system:String>
    <system:String x:Key="{x:Static reskeys:ResourceKeys.Menu_BreakPoint}">breakpoint</system:String>

