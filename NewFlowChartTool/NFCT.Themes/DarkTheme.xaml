<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  from AvalonDock.Themes.VS2013 reskeys:ResourceKeys.Background  -->
    <SolidColorBrush x:Key="BackGroundBrush" Color="#2D2D30" />
    <SolidColorBrush x:Key="ForeGroundBrush" Color="#FFFFFF" />
    <!--  used for icons from visual studio image library  -->
    <SolidColorBrush
        x:Key="light-defaultgrey"
        Opacity="1"
        Color="#F1F1F1" />
    <Style x:Key="TreeViewStyle" TargetType="{x:Type TreeView}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="ListViewStyle" TargetType="{x:Type ListView}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="GridStyle" TargetType="{x:Type Grid}">
        <Setter Property="Background" Value="Transparent" />
    </Style>
    <Style x:Key="LabelStyle" TargetType="{x:Type Label}">
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="TextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground" Value="White" />
    </Style>

</ResourceDictionary>